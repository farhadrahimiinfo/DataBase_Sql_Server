Create FUNCTION [dbo].[Julian_To_Gregorian] (@jdn bigint)
Returns nvarchar(11)
as
Begin
    Declare @Jofst  as Numeric(18,2)
    Set @Jofst=2415020.50
    --Return Convert(nvarchar(11),Convert(datetime,(@jdn- @Jofst),113),102)
    RETURN 
CONVERT(VARCHAR(4),YEAR (Convert(datetime,(@jdn- @Jofst),113)))
+'-'+
RIGHT('00'+CONVERT(VARCHAR(2),MONTH(Convert(datetime,(@jdn- @Jofst),113))),2)
+'-'+
RIGHT('00'+CONVERT(VARCHAR(2),DAY  (Convert(datetime,(@jdn- @Jofst),113))),2)
    
End
